{

    "manifest_version": 2,
    "name": "MyWebExt",
    "version": "0.0.1",
    "author": "MyName",
  
    "permissions": [
      "<all_urls>",
      "tabs"
    ],
  
    "content_security_policy": "script-src 'self'",
  
    "background": {
      "scripts": ["background_scripts/index.js"]
    },
  
    "content_scripts": [
      {
        "js": [
          "content_scripts/index.js"
        ]
      }
    ]
  }